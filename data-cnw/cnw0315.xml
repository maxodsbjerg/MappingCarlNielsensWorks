            <title type="alternative" xml:id="title_270N200B5" xml:lang="da">De dræbte Bjørnens Unger</title>
            <title type="alternative" xml:id="title_270N200BA" xml:lang="en">The bear's two cubs were murdered</title>
            <contributor xml:id="respStmt_939a3aee">
